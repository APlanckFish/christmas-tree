{
  "name": "christmas-tree",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:ws\" \"npm:dev:app\" --names \"WS,APP\" --prefix-colors \"cyan,green\"",
    "dev:app": "react-router dev",
    "dev:ws": "tsx ws-server.ts",
    "build": "react-router build",
    "start": "node --loader tsx/esm server.ts",
    "dev:custom": "WS_PORT=${WS_PORT:-8081} VITE_HTTP_PORT=${VITE_HTTP_PORT:-8080} npm run dev"
  },
  "dependencies": {
    "@mediapipe/tasks-vision": "^0.10.3",
    "@react-router/node": "^7.0.1",
    "@react-router/serve": "^7.0.1",
    "isbot": "^5.1.0",
    "qrcode": "^1.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.1",
    "three": "^0.160.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.0.1",
    "@types/node": "^20.11.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.160.0",
    "@types/ws": "^8.5.10",
    "concurrently": "^8.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
